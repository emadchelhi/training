   "source": [
    "def Kmer(s, k):\n",
    "    kmer = {}\n",
    "    for i in range(len(s)-k+1):\n",
    "        km = s[i:i+k]\n",
    "        if km in kmer:\n",
    "            kmer[km][0]+=1\n",
    "            kmer[km][1].append(i)\n",
    "        else:\n",
    "            kmer[km]=[1,[i]]\n",
    "    return kmer"
   ]
  },
