   "source": [
    "def selection_sort(A):\n",
    "    for i in range(0, len(A)):\n",
    "        min_pos = i\n",
    "        for j in range(i+1, len(A)):\n",
    "            if A[j] < A[min_pos]:\n",
    "                min_pos = j\n",
    "        A[i], A[min_pos] = A[min_pos], A[i]\n",
    "    return(A)\n",
    "    \n",
    "A=[5,2,4,1,6,3]\n",
    "selection_sort(A)"
   ]
  }
 ],
